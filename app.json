{
  "expo": {
    "name": "Pay Flow",
    "slug": "pay-flow",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#FFF",
      "resizeMode": "contain"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-google-signin/google-signin",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Pay Flow needs access to your Camera."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.6.20"
          }
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFF",
        "backgroundImage": "./assets/adaptive-icon.png"
      },
      "package": "com.gabriel85.payflow",
      "googleServicesFile": "./google-services.json",
      "permissions": ["android.permission.CAMERA"]
    },
    "ios": {
      "bundleIdentifier": "com.gabriel85.payflow",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSCameraUsageDescription": "Pay Flow needs access to your Camera."
      }
    },
    "extra": {
      "eas": {
        "projectId": "604c5ff7-433b-42f0-b408-076a16d88db6"
      }
    }
  }
}
